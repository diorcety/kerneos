<profile xmlns="org.ow2.kerneos:profile"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="org.ow2.kerneos:profile">

  <defaultRules>
    <rule role="jonas" policy="allow"/>
    <rule role="principal1" policy="deny"/>
  </defaultRules>

  <bundles>
    <bundle id="test1">
      <rules>
        <rule role="principal1" policy="allow"/>
      </rules>
      <services>
        <service id="test2">
          <rules>
            <rule role="principal1" policy="deny"/>
          </rules>
          <methods>
            <method id="test3">
              <rules>
                <rule role="principal1" policy="allow"/>
              </rules>
            </method>
          </methods>
        </service>
      </services>
    </bundle>
  </bundles>

</profile>
