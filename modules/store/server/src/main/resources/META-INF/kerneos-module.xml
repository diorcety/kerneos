<swf-module xmlns="org.ow2.kerneos:module"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="org.ow2.kerneos:module">
  <id>kerneos-store</id>
  <name>Kerneos Store</name>
  <description>This is a example</description>
  <small-icon>icons/icon16.png</small-icon>
  <big-icon>icons/icon64.png</big-icon>
  <file>store.swf</file>
  <services>
    <service id="hello_service"/>
    <service id="store_service"/>
  </services>
  <mappings>
    <mapping java="org.ow2.kerneos.modules.store.impl.StoreImpl"
             flex="org.ow2.kerneos.modules.store.vo.StoreVO"/>
    <mapping java="org.ow2.kerneos.modules.store.impl.ModuleImpl"
             flex="org.ow2.kerneos.modules.store.vo.ModuleVO"/>
    <mapping java="org.ow2.kerneos.modules.store.impl.CategoryImpl"
             flex="org.ow2.kerneos.modules.store.vo.CategoryVO"/>
  </mappings>
</swf-module>