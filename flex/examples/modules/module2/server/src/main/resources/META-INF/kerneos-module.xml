<swf-module xmlns="http://jasmine.ow2.org/xsds/kerneos-module-2.1.xsd"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://jasmine.ow2.org/xsds/kerneos-module-2.1.xsd">
  <id>kerneos-module2</id>
  <name>Kerneos Module 2</name>
  <description>A chat example using an asynchronous service</description>
  <small-icon>icons/icon16.png</small-icon>
  <big-icon>icons/icon64.png</big-icon>
  <file>module2.swf</file>
  <services>
    <service id="module2-serviceSync"/>
    <service id="module2-serviceAsync" type="asynchronous"/>
  </services>
  <mappings>
    <mapping java="org.ow2.kerneos.examples.modules.module2.Post"
             flex="org.ow2.kerneos.examples.modules.module2.vo.PostVO"/>
  </mappings>
</swf-module>