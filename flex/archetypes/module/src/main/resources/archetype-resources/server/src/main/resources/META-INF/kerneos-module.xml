<?xml version="1.0" encoding="UTF-8"?>
<!-- Module settings -->
<!-- Example :-->
<swf-module xmlns="http://jasmine.ow2.org/xsds/kerneos-module-2.1.xsd"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://jasmine.ow2.org/xsds/kerneos-module-2.1.xsd">
  <id>hello-service</id>
  <name>Hello Service</name>
  <description>This is a example</description>
  <small-icon>icons/icon16.png</small-icon>
  <big-icon>icons/icon64.png</big-icon>
  <file>example.swf</file>
  <services>
    <service id="hello_service"/>
  </services>
</swf-module>

    <!-- Note:
   The file name "example.swf" has to be replaced into the server pom.xml
   <destFileName>example.swf</destFileName> -->
